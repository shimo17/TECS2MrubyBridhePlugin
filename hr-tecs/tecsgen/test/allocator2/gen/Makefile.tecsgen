TECS_IMPORT_CDLS = allocator.cdl gen/tmp_plugin_post_code.cdl
TECS_IMPORT_HEADERS = ../RUNTIME/tecs-runtime/include/tecs.h
TECS_IMPORTS = $(TECS_IMPORT_CDLS) $(TECS_IMPORT_HEADERS)

SIGNATURE_HEADERS = \
	$(GEN_DIR)/sAlloc_tecsgen.h \
	$(GEN_DIR)/sAllocTMO_tecsgen.h \
	$(GEN_DIR)/sSendRecv_tecsgen.h \
# SIGNATURE_HEADERS terminator

CELLTYPE_TECSGEN_HEADERS = \
 	$(GEN_DIR)/tAlloc_tecsgen.h \
 	$(GEN_DIR)/tRelayComponent_tecsgen.h \
 	$(GEN_DIR)/tTargetComponent_tecsgen.h \
 	$(GEN_DIR)/tTestClient_tecsgen.h \
 	$(GEN_DIR)/tTestOptional_tecsgen.h \
# CELLTYPE_TECSGEN_HEADERS terminator

CELLTYPE_FACTORY_HEADERS = \
 	$(GEN_DIR)/tAlloc_factory.h \
 	$(GEN_DIR)/tRelayComponent_factory.h \
 	$(GEN_DIR)/tTargetComponent_factory.h \
 	$(GEN_DIR)/tTestClient_factory.h \
 	$(GEN_DIR)/tTestOptional_factory.h \
# CELLTYPE_FACTORY_HEADERS terminator

# TECS_HEADERS:  headers generated by tecsgen
TECS_HEADERS = $(SIGNATURE_HEADERS) $(CELLTYPE_TECSGEN_HEADERS) $(CELLTYPE_FACTORY_HEADERS)

TECS_INLINE_HEADERS = \
 	tAlloc_tecsgen.h \
 	tRelayComponent_tecsgen.h \
 	tTargetComponent_tecsgen.h \
 	tTestClient_tecsgen.h \
 	tTestOptional_tecsgen.h \
# TECS_INLINE_HEADERS terminator

PLUGIN_INLINE_HEADERS = \
# PLUGIN_INLINE_HEADERS terminator

# TECS_COBJS: all objects of TECS, include both user written code and tecsgen automatically generated code
TECS_COBJS = $(TECSGEN_COBJS) $(PLUGIN_COBJS) $(CELLTYPE_COBJS)

# TECS_COBJS: objects from sources which are automatically generated by 
      
TECSGEN_COBJS = \
 	$(_TECS_OBJ_DIR)tAlloc_tecsgen.o \
 	$(_TECS_OBJ_DIR)tRelayComponent_tecsgen.o \
 	$(_TECS_OBJ_DIR)tTargetComponent_tecsgen.o \
 	$(_TECS_OBJ_DIR)tTestClient_tecsgen.o \
 	$(_TECS_OBJ_DIR)tTestOptional_tecsgen.o \
# TECSGEN_COBJS terminator

# PLUGIN_COBJS: objects from sources which are automatically generated by plugin(s)
PLUGIN_COBJS = \
# PLUGIN_COBJS terminator

# CELLTYPE_COBJS: objects of celltype code written by user
CELLTYPE_COBJS = \
 	$(_TECS_OBJ_DIR)tAlloc.o \
 	$(_TECS_OBJ_DIR)tRelayComponent.o \
 	$(_TECS_OBJ_DIR)tTargetComponent.o \
 	$(_TECS_OBJ_DIR)tTestClient.o \
 	$(_TECS_OBJ_DIR)tTestOptional.o \
# CELLTYPE_COBJS terminator

# TECSGEN_SRCS: sources automatically generated by tecsgen
TECSGEN_SRCS = \
 	$(GEN_DIR)/tAlloc_tecsgen.c \
 	$(GEN_DIR)/tRelayComponent_tecsgen.c \
 	$(GEN_DIR)/tTargetComponent_tecsgen.c \
 	$(GEN_DIR)/tTestClient_tecsgen.c \
 	$(GEN_DIR)/tTestOptional_tecsgen.c \
# TECSGEN_SRCS terminator

# PLUGIN_SRCS: sources automatically generated by plugin
PLUGIN_SRCS = \
# PLUGIN_SRCS terminator

