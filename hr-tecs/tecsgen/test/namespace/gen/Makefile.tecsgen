TECS_HEADERS = $(SIGNATURE_HEADERS) $(CELLTYPE_TECSGEN_HEADERS) $(CELLTYPE_FACTORY_HEADERS)

TECS_INLINE_HEADERS = \
 	tKernel_tecsgen.h \
 	tAppl_tecsgen.h \
 	tOSEKAppl_tecsgen.h \
 	nAlloc_tAlloc_tecsgen.h \
