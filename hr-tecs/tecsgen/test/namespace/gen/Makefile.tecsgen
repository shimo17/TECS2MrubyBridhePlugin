TECS_IMPORT_HEADERS = ../cygwin/cygwin_tecs.h
TECS_IMPORTS = $(TECS_IMPORT_CDLS) $(TECS_IMPORT_HEADERS)

SIGNATURE_HEADERS = \
	$(GEN_DIR)/sSyscall_tecsgen.h \
