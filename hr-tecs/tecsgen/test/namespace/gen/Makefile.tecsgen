 	$(GEN_DIR)/tKernel_tecsgen.h \
 	$(GEN_DIR)/tAppl_tecsgen.h \
 	$(GEN_DIR)/tOSEKAppl_tecsgen.h \
 	$(GEN_DIR)/nAlloc_tAlloc_tecsgen.h \
 	$(GEN_DIR)/TOPPERS_tKernel_tecsgen.h \
